{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name":"client",
      "image": "jstanley1730/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name":"nginx",
      "image": "jstanley1730/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "client",
        "server"
      ]
    },
    {
      "name":"server",
      "image": "jstanley1730/multi-server",
      "hostname": "server",
      "essential": false
    },
    {
      "name":"worker",
      "image": "jstanley1730/multi-worker",
      "hostname": "worker",
      "essential": false
    }
  ]
}